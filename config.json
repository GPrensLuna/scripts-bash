{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit",
    "source.addMissingImports.ts": "explicit",
    "source.organizeImports.ts": "explicit",
    "source.fixAll.markdown": "explicit",
    "source.fixAll.json": "explicit",
    "source.sortImports": "explicit",
    "source.fixAll.yaml": "explicit",
    "editor.formatOnFocus": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "eslint.codeActionsOnSave.mode": "all",
  "files.eol": "\n",


  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "json",
    "markdown",
    "yaml"
  ],
  //editor
  "editor.acceptSuggestionOnEnter": "on", // Aceptar sugerencias con Enter o Tab
  "editor.tabCompletion": "on", // Completar automáticamente con Tab
  "editor.snippetSuggestions": "inline", // Estilo del cursor (en línea, bloque, etc.)
  "editor.autoIndent": "full", // Indentación automática completa
  "editor.fontFamily": "Fira Code Light",
  "editor.fontLigatures": true, // Ligaduras tipográficas
  "editor.lightbulb.enabled": "on",
  "editor.fontSize": 16,
  "editor.lineHeight": 18,
  "editor.letterSpacing": 0.1,
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 2,
  "editor.minimap.maxColumn": 90,
  "editor.minimap.enabled": false,
  "editor.fastScrollSensitivity": 2,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.cursorSurroundingLines": 3,
  "editor.rulers": [90], // Línea visual a las 80 columnas
  "editor.wordWrap": "bounded", // Ajusta el texto a la ventana sin sobrepasar el límite
  "editor.wordWrapColumn": 90,
  //editor -> workbench
  "workbench.settings.applyToAllProfiles": [
    "editor.unfoldOnClickAfterEndOfLine",
    "prettier.endOfLine",
    "autoBarrel.files.detectExportsInFiles",
    "autoBarrel.files.disableRecursiveBarrelling",
    "autoBarrel.files.keepExtensionOnExport",
    "autoBarrel.formatting.excludeSemiColonAtEndOfLine",
    "cSpell.autoFormatConfigFile",
    "cSpell.showAutocompleteSuggestions",
    "cSpell.validateDirectives",
    "console-ninja.captureFunctions",
    "errorLens.codeLensEnabled",
    "errorLens.enableOnDiffView",
    "eslint.debug",
    "eslint.format.enable",
    "git.autoStash",
    "git.fetchOnPull",
    "jest.debugMode",
    "material-icon-theme.enableLogging",
    "prettier.enableDebugLogs",
    "explorer.excludeGitIgnore",
    "editor.codeActionsOnSave",
    "files.autoSave"
  ],
  //terminal
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontFamily": "CaskaydiaMono Nerd Font Mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.letterSpacing": 0,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.env.linux": {},
  // extensions
  "tabnine.codeLensEnabled": true,
  "workbench.productIconTheme": "emoji-product-icons-fun",
  "database-client.autoSync": true,
  // extension -> material icon
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    //applications
    "application": "app",
    "applications": "app",
    "use-case": "controller",
    "use-cases": "controller",
    "useCase": "controller",
    "useCases": "controller",
    //domain
    "domain": "global",
    "domains": "global",
    "entity": "database",
    "entities": "database",
    "repositories": "constant",
    //infrastructure
    "infrastructure": "batch",
    "infrastructures": "batch",
    "controllers": "controller",
    // dtos
    "user": "bicep",
    "dto": "resource",
    "dtos": "resource",
    "request": "container",
    "response": "resolver",
    //shared
    "logger": "log",
    //common
    "interceptors": "context",
    //other
    "migrations": "jinja_light",
    "Redux": ""
  },
  "material-icon-theme.files.associations": {
    //applications
    "*.use-case.ts": "nest-service",
    "*.useCase.ts": "nest-service",
    //domain
    "*.entity.ts": "database",
    "*.entities.ts": "database",
    "*.repository.ts": "payload_light",
    "*.api.ts": "payload_light",
    "*.interface.ts": "document",
    //infrastructure
    "*.doc.ts": "document",
    "*.documentation.ts": "document",
    // dtos
    "*.dto.ts": "contributing",
    //shared
    //common
    "*.strategy.ts": "lock",
    //other
    "*.config.ts": "settings",
    "*.schema.ts": "database"
  },
  //idioma
  "cSpell.language": "en,es,es-ES",
