  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "top",
  "cSpell.autoFormatConfigFile": false,
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.validateDirectives": true,
  "material-icon-theme.enableLogging": true,
  "search.useGlobalIgnoreFiles": true,
  "tabnine.experimentalAutoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.empty.hint": "hidden",
  "telemetry.telemetryLevel": "off",
  "database-client.telemetry.usesOnlineServices": false,
  "rest-client.enableTelemetry": false,
  "editor.cursorSmoothCaretAnimation": "explicit",
  "window.menuBarVisibility": "compact",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "oneDarkPro.vivid": true,
  "oneDarkPro.bold": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.guides.highlightActiveIndentation": "always",
  "better-comments.highlightPlainText": true,
  "database-client.highlightSQLBlock": true,
  "editor.language.colorizedBracketPairs": [],
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.customLabels.patterns": {
    "**/components/**": "${filename} - 🧩",
    "**/page/**": "${filename} - 📝",
    "**/pages/**": "${filename} - 📝",
    "**/ui/pages/**": "${filename} - 📝",
    "**/ui/assert/**": "${filename} - ⚡",
    "**/ui/hooks/**": "${filename} - 🔌",
    "**/ui/repositories/**": "${filename} - 🗂️",
    "**/application/dtos/**": "${filename} - 📄",
    "**/application/use-case/**": "${filename} - 💼",
    "**/domain/entities/**": "${filename} - 🏛️",
    "**/domain/interfaces/**": "${filename} - 📐",
    "**/domain/services/**": "${filename} - 🛠️",
    "**/infrastructure/controllers/**": "${filename} - 🎮",
    "**/infrastructure/repositories/**": "${filename} - 🏗️",
    "**/tests/integration/**": "${filename} - 🔍",
    "**/tests/unit/**": "${filename} - 🧪",
    "**/UI/components/**": "${filename} - 🧩",
    "**/UI/hooks/**": "${filename} - 🔌",
    "**/UI/context/**": "${filename} - 🧑‍🤝‍🧑",
    "**/UI/config/**": "${filename} - ⚙️",
    "**/UI/tests/unit/**": "${filename} - 🧪",
    "**/UI/tests/integration/**": "${filename} - 🔍",
    "**/UI/page.tsx": "${filename} - 📝"
  },

  "editor.linkedEditing": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, yarn.lock,postcss.config.mjs,tsconfig.tsbuildinfo, components.json, pnpm-lock.yaml, bun.lockb, postcss.config.js, tailwind.config.ts, tsconfig.json, eslint.config.mjs, .gitignore, .editorconfig, .lintstagedrc, next-env.d.ts, next.config.ts,LICENSE, env.d.ts,.env,README.md, validateEnv.ts,next-auth.d.ts",
    "src/": "*.js, *.ts, *.jsx, *.tsx, *.css, *.scss, *.html, *.json",
    "components/": "*.js, *.ts, *.jsx, *.tsx, *.css, *.scss, *.html, *.json",
    "pages/": "*.js, *.ts, *.jsx, *.tsx, *.css, *.scss, *.html, *.json",
    "utils/": "*.js, *.ts, *.jsx, *.tsx, *.css, *.scss, *.html, *.json",
    "styles/": "*.css, *.scss",
    "assets/": "*.png, *.jpg, *.jpeg, *.gif, *.svg, *.ico",
    "config/": "*.json, *.js, *.ts",
    "tests/": "*.spec.js, *.spec.ts, *.test.js, *.test.ts"
  },
  "editor.stickyTabStops": true,
  "workbench.editor.wrapTabs": true,
  "editor.renderWhitespace": "all",
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeium.enableConfig": {
    "*": true,
    "git-commit": true
  },
  "git.autofetch": true,
  "console-ninja.featureSet": "Community",
  "console-ninja.captureFunctions": true,
  "errorLens.codeLensEnabled": true,
  "errorLens.enableOnDiffView": true,
  "console-flash.double-quotes": true,
  "console-flash.semi-colon": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overtypeCursorStyle": "line",
  "editor.overtypeOnPaste": false,
  "window.customTitleBarVisibility": "auto",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "files.autoSave": "onWindowChange",
  "files.associations": {
    "*.ts": "typescriptreact"
  },
  "editor.unfoldOnClickAfterEndOfLine": true,
  "explorer.confirmDragAndDrop": false
}
