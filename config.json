{
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always",
    "source.addMissingImports.ts": "always",
    "source.organizeImports.ts": "always",
    "source.fixAll.markdown": "always",
    "source.fixAll.json": "always",
    "source.sortImports": "always",
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "json",
    "markdown",
    "yaml"
  ],
  "typescript.autoImportSuggestions.enabled": true,

  "eslint.alwaysShowStatus": true,
  "editor.formatOnSaveMode": "file",
  "prettier.requireConfig": true,
  //editor
  "editor.fontFamily": "CaskaydiaCove Nerd Font Mono,Cascadia Mono",
  "editor.fontLigatures": true,
  "editor.lightbulb.enabled": "on",
  "editor.fontSize": 16,
  "editor.lineHeight": 18,
  "editor.letterSpacing": 0.1,
  "editor.wordWrap": "wordWrapColumn",
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.cursorWidth": 2,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "eslint.codeActionsOnSave.mode": "all",
  "editor.minimap.renderCharacters": false,
  "editor.fastScrollSensitivity": 2,
  "editor.bracketPairColorization.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.cursorSurroundingLines": 3,
  //editor -> workbench
  "files.eol": "\n",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.applyToAllProfiles": [
    "editor.unfoldOnClickAfterEndOfLine",
    "prettier.endOfLine",
    "autoBarrel.files.detectExportsInFiles",
    "autoBarrel.files.disableRecursiveBarrelling",
    "autoBarrel.files.keepExtensionOnExport",
    "autoBarrel.formatting.excludeSemiColonAtEndOfLine",
    "cSpell.autoFormatConfigFile",
    "cSpell.showAutocompleteSuggestions",
    "cSpell.validateDirectives",
    "console-ninja.captureFunctions",
    "errorLens.codeLensEnabled",
    "errorLens.enableOnDiffView",
    "eslint.debug",
    "eslint.format.enable",
    "git.autoStash",
    "git.fetchOnPull",
    "jest.debugMode",
    "material-icon-theme.enableLogging",
    "prettier.enableDebugLogs",
    "explorer.excludeGitIgnore"
  ],
  //terminal
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font Mono",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.letterSpacing": 0,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.env.linux": {},
  // extensions
  "tabnine.codeLensEnabled": true,
  "workbench.productIconTheme": "emoji-product-icons-fun",
  "database-client.autoSync": true,
  // extension -> material icon
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    //applications
    "application": "app",
    "applications": "app",
    "use-case": "controller",
    "use-cases": "controller",
    //domain
    "domain": "global",
    "entity": "database",
    "entities": "database",
    "repositories": "constant",
    //infrastructure
    "infrastructure": "batch",
    "controllers": "controller",
    // dtos
    "user": "bicep",
    "dto": "resource",
    "dtos": "resource",
    "request": "container",
    "response": "resolver",
    //shared
    "logger": "log",
    //common
    "interceptors": "context",
    //other
    "migrations": "jinja_light"
  },
  "material-icon-theme.files.associations": {
    //applications
    "*.use-case.ts": "nest-service",
    //domain
    "*.entity.ts": "database",
    "*.entities.ts": "database",
    "*.repository.ts": "payload_light",
    "*.api.ts": "payload_light",
    "*.interface.ts": "document",
    //infrastructure
    "*.doc.ts": "document",
    "*.documentation.ts": "document",
    // dtos
    "*.dto.ts": "contributing",
    //shared
    //common
    "*.strategy.ts": "lock",
    //other
    "*.config.ts": "settings",
    "*.schema.ts": "database"
  },
  //idioma
  "cSpell.language": "en,es,es-ES",
  "cSpell.userWords": [
    "appncy",
    "Cascadia",
    "Caskaydia",
    "ciclomática",
    "cloudinary",
    "clsx",
    "compat",
    "compilaciónlimina",
    "compilaciónvita",
    "dbaeumer",
    "dtos",
    "dxmqlmslg",
    "esbenp",
    "esnext",
    "fieldname",
    "friday",
    "Giovanny",
    "heroicons",
    "hookform",
    "identar",
    "indentación",
    "inicialización",
    "interoperabilidad",
    "iteradores",
    "jinja",
    "Kamil",
    "lucide",
    "monday",
    "multer",
    "Myśliwiec",
    "nestframework",
    "nestjs",
    "noopener",
    "noreferrer",
    "nullabilidad",
    "opentelemetry",
    "otel",
    "Parens",
    "pnpx",
    "prens",
    "preprocesador",
    "redirecciones",
    "reduxjs",
    "rvest",
    "serviceworker",
    "sherlock",
    "sqlite",
    "Swal",
    "sweetalert",
    "tabnine",
    "tabulación",
    "tabulaciones",
    "tailwindcss",
    "trpc",
    "tseslint",
    "tsup",
    "tuesday",
    "usecase",
    "wednesday"
  ],
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "bottom",
  "cSpell.autoFormatConfigFile": false,
  "cSpell.showAutocompleteSuggestions": true,
  "cSpell.validateDirectives": true,
  "material-icon-theme.enableLogging": true,
  "search.useGlobalIgnoreFiles": true,
  "explorer.excludeGitIgnore": true,
}
